          </xs:complexType>
        </xs:element>
      </xs:choice>
      <xs:attribute name="Id" type="xs:string" use="required" />
      <xs:attribute name="Type" use="required" >
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Lru"/>
            <xs:enumeration value="Fifo"/>
